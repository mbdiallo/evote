<div class="block-header">
  <h2>Votes</h2>
</div>

<%= render partial: 'shared/notices'%>

<div class="card">
  <div class="listview lv-bordered lv-lg">
    <div class="lv-header-alt clearfix">
      <h2 class="center">Click on the picture to vote </h2>

      <div class="pull-right">

      </div>
    </div>

    <div class="lv-body">
      <div class="media normal">
        <% @results.each do |result| %>
          <h1><%= link_to 'Edit', edit_result_path(result) %></h1>
        <% end %>
      </div>
    </div>
  </div>
</div>
